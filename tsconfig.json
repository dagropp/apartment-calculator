{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUnusedLocals": true,
    "allowUnreachableCode": true,
    "skipLibCheck": true
  },
  "include": [
    "./*",
    "src/DoDom/*",
    "src/*"
  ]
}
